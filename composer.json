{
    "name": "amber/amber",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Deivi Pe√±a",
            "email": "systemson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/systemson/route"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Amber\\Framework\\": "framework"
        },
        "files": [
            "config/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2.0",
        "amber/cache": "dev-master",
        "amber/common": "dev-master",
        "amber/container": "dev-master",
        "amber/route": "dev-master",
        "illuminate/database": "^5.8",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "filp/whoops": "^2.3",
        "symfony/var-dumper": "^4.2"
    },
    "scripts": {},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "process-timeout": 0
    }
}
